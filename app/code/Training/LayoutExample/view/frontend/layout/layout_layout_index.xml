<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Manthan Shinde</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="title" xsi:type="string">Manthannn</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block name="layout" class="Magento\Framework\View\Element\Template" template="Training_LayoutExample::content.phtml" />    
        </referenceContainer>
    </body>
</page>
